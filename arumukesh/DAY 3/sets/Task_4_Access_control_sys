prod_access = {"John", "Priya", "Amit", "Neha"}
test_access = {"Amit", "Neha", "Rahul", "Sita"}
print("employees with access to both prod and test systems:", prod_access.intersection(test_access))
print("employees with access to atleast one server :", prod_access.union(test_access))
print("employees with access to only prod:", prod_access.difference(test_access))
prod_access.add("Rahul")
print("Updated prod:",prod_access)
prod_access.discard("Rahul")

print("Final sorted acces list:",sorted(prod_access.union(test_access)))

# employees with access to both prod and test systems: {'Amit', 'Neha'}
# employees with access to atleast one server : {'Priya', 'Rahul', 'Amit', 'John', 'Sita', 'Neha'}
# employees with access to only prod: {'Priya', 'John'}
# Updated prod: {'Priya', 'Rahul', 'Amit', 'John', 'Neha'}
# Final sorted acces list: ['Amit', 'John', 'Neha', 'Priya', 'Rahul', 'Sita']
