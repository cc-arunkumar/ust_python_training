servers = (
 ("AppServer1", ("192.168.1.10", 65, "Running")),
 ("DBServer1", ("192.168.1.11", 85, "Running")),
 ("CacheServer", ("192.168.1.12", 90, "Down")),
 ("BackupServer", ("192.168.1.13", 40, "Running"))
)

for server_name, (ip, cpu, status) in servers:
    if status == "Running":
        print(f"Server: {server_name}, IP: {ip}, CPU Usage: {cpu}%")
print("Server with highest cpu usage:", max(servers, key=lambda x: x[1][1]))
f_list=list(filter(lambda x:x[1][1]>80,servers))
print("Servers with CPU usage greater than 80%:",f_list)

# Server: AppServer1, IP: 192.168.1.10, CPU Usage: 65%
# Server: DBServer1, IP: 192.168.1.11, CPU Usage: 85%
# Server: BackupServer, IP: 192.168.1.13, CPU Usage: 40%
# Server with highest cpu usage: ('CacheServer', ('192.168.1.12', 90, 'Down'))
# Servers with CPU usage greater than 80%: [('DBServer1', ('192.168.1.11', 85, 'Running')), ('CacheServer', ('192.168.1.12', 90, 'Down'))]
    