connected_ips = {"10.0.0.1", "10.0.0.2", "10.0.0.5", "10.0.0.8", "10.0.0.9"}
blacklisted_ips = {"10.0.0.2", "10.0.0.8", "10.0.0.10"}
print("Blacklisted but connected",connected_ips.union(blacklisted_ips))
print("Safe connected IPs",connected_ips.difference(blacklisted_ips))
blacklisted_ips.add("10.0.0.9")
for ip in connected_ips:
    if ip in blacklisted_ips:
        print(f"Alert! Blacklisted IP connected: {ip}")
print("total blacklisted IPs:",len(blacklisted_ips))

# Blacklisted but connected {'10.0.0.5', '10.0.0.1', '10.0.0.9', '10.0.0.8', '10.0.0.10', '10.0.0.2'}
# Safe connected IPs {'10.0.0.5', '10.0.0.1', '10.0.0.9'}
# Alert! Blacklisted IP connected: 10.0.0.9
# Alert! Blacklisted IP connected: 10.0.0.8
# Alert! Blacklisted IP connected: 10.0.0.2
# total blacklisted IPs: 4
